{
  "private": false,
  "name": "energy-manager-screen-capture",
  "version": "0.0.13",
  "description": "Screen capture tool",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/screen-capture-button.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/screen-capture-button.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/screen-capture-button.min.js"
  },
  "author": "energy-manager",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "rollup": "^1.17.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "html2canvas": "^1.0.0-rc.7"
  }
}
